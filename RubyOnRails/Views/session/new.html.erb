<header class="masthead clear">
  <div class="centered">
    <div class="site-branding">
      <h1 class="site-title">Welcome visitor!</h1>
    </div>
  </div>
</header>

<div id="login-form">
  <%= form_for :session, url: {action: "create"} do |t| %>
    <div class="cont">
      <div class="field">
        <%= t.label :username %>
        <%= t.label :password %>
      </div>
      <div class="field">
        
        <%= t.text_field :username, required: true %>
        <%= t.password_field :password, required: true %>
      </div>
    </div>
    <div class="actions">
      <%= t.submit 'Log In' %>
    </div>

  <% end %>
</div>


